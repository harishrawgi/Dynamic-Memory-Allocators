   "source": [
    "import scipy.stats as stats\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "lower, upper, scale = 100, 2000, 1000\n",
    "X = stats.truncexpon(b=(upper-lower)/scale, loc=lower, scale=scale)\n",
    "\n",
    "data = np.asarray(X.rvs(1000))\n",
    "\n",
    "\n",
    "print(data)\n",
    "\n",
    "np.savetxt('exp.txt', data)"
   ]
  }
